Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/emu
  IdentityFile ~/.ssh/github

# GitHub.
Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes

# GitHub EMU.
Host emu
    HostName github.com
    User git
    IdentityFile ~/.ssh/emu
    IdentitiesOnly yes
