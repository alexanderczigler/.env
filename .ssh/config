# GitHub + GitLab
Match Host git*b.com
  IdentityFile ~/.ssh/git_rsa
  IdentitiesOnly yes

# SEB internal GitHub/TFS
Match Host *bank.se
  IdentityFile ~/.ssh/git_rsa
  IdentitiesOnly yes

